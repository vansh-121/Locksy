{
  "manifest_version": 3,
  "name": "Secure Tab Extension",
  "version": "6.1",
  "description": "Optimized secure tab protection with unbypassable lock overlay, page blur, and efficient multi-layer defense against inspect element bypass. Zero lag performance.",
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "activeTab",
    "notifications",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}