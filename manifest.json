{
  "manifest_version": 3,
  "name": "Tab Lock Extension",
  "version": "1.0",
  "description": "Lock and unlock tabs with a password for secure browsing.",
  "permissions": ["storage", "tabs", "scripting", "identity", "activeTab", "notifications"],
  "oauth2": {
  "client_id": "590193531184-abfrknjc7k1kvhj8ke6r4qqf7mq0klrn.apps.googleusercontent.com",
  "scopes": ["email", "profile"]
},
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
